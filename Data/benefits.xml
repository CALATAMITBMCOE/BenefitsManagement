    <instance instanceCode="BEN00106" objectCode="benf_calc">
      <CustomInformation>
        <ColumnValue name="partition_code">NIKU.ROOT</ColumnValue>
        <ColumnValue name="name">Improved Portfolio Management due to Reduction in Low-Value Projects</ColumnValue>
