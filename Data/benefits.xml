    <instance instanceCode="BEN00100" objectCode="benf_calc">
      <CustomInformation>
        <ColumnValue name="partition_code">NIKU.ROOT</ColumnValue>
        <ColumnValue name="name">Time Savings from Reduced or Shorter Meetings</ColumnValue>
