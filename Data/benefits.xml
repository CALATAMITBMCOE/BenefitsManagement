          <Value>EN007</Value>
          <Value>EN006</Value>
          <Value></Value>
          <Value></Value>
          <Value></Value>
          <Value></Value>
          <Value></Value>
        </ColumnValue>
        <ColumnValue name="benf_recurrency_per">A</ColumnValue>
      </CustomInformation>
      <OBSAssocs complete="false"/>
